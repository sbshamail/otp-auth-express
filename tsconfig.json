{
  "compilerOptions": {
    "target": "ES2024",
    "module": "commonjs",
    "outDir": "dist",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "forceConsistentCasingInFileNames": false,
    "noImplicitAny": false
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
